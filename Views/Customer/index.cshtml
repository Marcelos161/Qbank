@model IEnumerable<MeuProjetoMVC.Models.Customer>
<h2>Clientes</h2>
<table>
    <thead>
        <tr>
            <th>Nome</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var customer in Model)
    {
        <tr>
            <td>@customer.Name</td>
            <td>@customer.Email</td>
        </tr>
    }
    </tbody>
</table>